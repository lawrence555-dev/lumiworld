{
    "curriculum": {
        "totalWeeks": 8,
        "targetAge": 4,
        "focusAreas": [
            "STEAM",
            "Early Literacy",
            "Science",
            "Math",
            "SEL"
        ],
        "weeks": [
            {
                "weekNumber": 1,
                "title": "Scientific Classification",
                "theme": "Living vs. Non-Living",
                "standards": [
                    "NGSS K-LS1-1"
                ],
                "learningObjectives": [
                    "Distinguish between living and non-living things",
                    "Identify requirements for life",
                    "Classify objects by biological attributes"
                ],
                "vocabulary": [
                    "Living",
                    "Non-living",
                    "Energy",
                    "Growth"
                ]
            },
            {
                "weekNumber": 2,
                "title": "Introduction to Anatomy",
                "theme": "Fish Body Parts & Phonics",
                "standards": [
                    "CCSS.ELA-LITERACY.RF.K.3.A"
                ],
                "learningObjectives": [
                    "Identify external anatomy of a fish (fin, tail, gills)",
                    "Recognize letter F sound/shape",
                    "Assemble biological puzzles"
                ],
                "vocabulary": [
                    "Fish",
                    "Fin",
                    "Tail",
                    "Gills",
                    "Scale"
                ]
            },
            {
                "weekNumber": 3,
                "title": "Number Sense & Needs",
                "theme": "Counting & Feeding",
                "standards": [
                    "CCSS.MATH.CONTENT.K.CC.B.4"
                ],
                "learningObjectives": [
                    "Count items 1-5 with one-to-one correspondence",
                    "Match quantity to numerical symbols",
                    "Understand feeding as a biological need"
                ],
                "vocabulary": [
                    "One",
                    "Two",
                    "Three",
                    "Four",
                    "Five",
                    "Feed"
                ]
            },
            {
                "weekNumber": 4,
                "title": "Comparative Measurement",
                "theme": "Elephant & Mouse (Size)",
                "standards": [
                    "CCSS.MATH.CONTENT.K.MD.A.2"
                ],
                "learningObjectives": [
                    "Compare objects using big/small/heavy/light",
                    "Identify relative size in animals",
                    "Use comparative vocabulary"
                ],
                "vocabulary": [
                    "Big",
                    "Small",
                    "Elephant",
                    "Mouse",
                    "Compare"
                ]
            },
            {
                "weekNumber": 5,
                "title": "Habitat Exploration",
                "theme": "Eagle Perspective",
                "standards": [
                    "NGSS/CCSS Spatial Logic"
                ],
                "learningObjectives": [
                    "Understand habitats from different perspectives",
                    "Use spatial prepositions (up/down/between)",
                    "Identify bird habitats"
                ],
                "vocabulary": [
                    "Eagle",
                    "Sky",
                    "Habitat",
                    "Up",
                    "Down"
                ]
            },
            {
                "weekNumber": 6,
                "title": "Botany Basics",
                "theme": "Plant Growth Cycles",
                "standards": [
                    "NGSS Life Cycles"
                ],
                "learningObjectives": [
                    "Sequence plant growth from seed to flower",
                    "Identify plant needs (sun, water, soil)",
                    "Understand growth as a time-based process"
                ],
                "vocabulary": [
                    "Seed",
                    "Soil",
                    "Water",
                    "Sun",
                    "Root",
                    "Stem"
                ]
            },
            {
                "weekNumber": 7,
                "title": "Environmental Science",
                "theme": "Ocean Pollution & SEL",
                "standards": [
                    "NGSS K-ESS3-3",
                    "CASEL SEL"
                ],
                "learningObjectives": [
                    "Identify human impact on marine environments",
                    "Recognize emotions (sad/happy) in affected animals",
                    "Classify trash vs. natural objects"
                ],
                "vocabulary": [
                    "Pollution",
                    "Trash",
                    "Dolphin",
                    "Save",
                    "Clean"
                ]
            },
            {
                "weekNumber": 8,
                "title": "Capstone Project",
                "theme": "Ecosystem Management",
                "standards": [
                    "STEAM Integration"
                ],
                "learningObjectives": [
                    "Apply multi-week knowledge to a system",
                    "Balance needs of multiple organisms",
                    "Demonstrate executive function in problem solving"
                ],
                "vocabulary": [
                    "Ecosystem",
                    "Balance",
                    "Hero",
                    "Helper",
                    "Care"
                ]
            }
        ]
    }
}